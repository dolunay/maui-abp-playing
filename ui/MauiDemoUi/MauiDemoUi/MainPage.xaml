<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:b="clr-namespace:Microsoft.AspNetCore.Components.WebView.Maui;assembly=Microsoft.AspNetCore.Components.WebView.Maui"
             xmlns:local="clr-namespace:MauiDemoUi"
             xmlns:basictheme="clr-namespace:Volo.Abp.AspNetCore.Components.Web.BasicTheme.Themes.Basic;assembly=Volo.Abp.AspNetCore.Components.Web.BasicTheme"
             x:Class="MauiDemoUi.MainPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}">

    <b:BlazorWebView HostPage="wwwroot/index.html">
        <b:BlazorWebView.RootComponents>
            <b:RootComponent Selector="#app" ComponentType="{x:Type basictheme:App}" />
        </b:BlazorWebView.RootComponents>
    </b:BlazorWebView>

</ContentPage>